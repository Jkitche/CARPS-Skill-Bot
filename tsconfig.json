{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"module": "commonjs",
		"target": "es6",
		"resolveJsonModule": true,
		"noImplicitAny": true,
		"moduleResolution": "node",
		"esModuleInterop": true
	},
	"exclude": [
		"node_modules",
		"**/*test.js"
	],
	"include": ["src/**/*"]
}
